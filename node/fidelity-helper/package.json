{
  "name": "fidelity-helper",
  "version": "0.0.1",
  "packageManager": "yarn@4.12.0",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "yarn lint && tsc",
    "clean": "rm -rf dist && rm -f package.tgz",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "yarn vitest",
    "type-check": "tsc --project tsconfig.test.json",
    "package": "yarn clean && yarn build && tar -czf package.tgz --dereference --transform 's,^,package/,' dist src fidelity-templates LICENSE README.md package.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/mustache": "^4",
    "@types/node": "^25.0.3",
    "eslint": "^9.35.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "mustache": "^4.2.0"
  }
}
